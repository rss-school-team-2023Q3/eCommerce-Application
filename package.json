{
  "name": "ecommerce-application",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev && npm run storybook",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src  --check",
    "prettier:fix": "npm run prettier -- --write",
    "style": "npx stylelint \"**/*.css\"",
    "style:fix": "npx stylelint \"**/*.css\" --fix",
    "format": "npm run prettier:fix && npm run lint:fix && npm run style:fix",
    "preview": "vite preview",
    "prepare": "husky",
    "cm": "cz",
    "test": "cross-env CI=true vitest",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6007",
    "build-storybook": "storybook build",
    "generate": "plop component"
  },
  "dependencies": {
    "@commercetools/history-sdk": "^4.3.0",
    "@commercetools/importapi-sdk": "^5.4.0",
    "@commercetools/platform-sdk": "^7.8.0",
    "@commercetools/sdk-client-v2": "^2.5.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@mui/material": "^5.15.16",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@vitest/ui": "^1.6.0",
    "editorconfig": "^2.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "jest-dom": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-node-polyfills": "^0.2.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@storybook/addon-essentials": "^8.0.10",
    "@storybook/addon-interactions": "^8.0.10",
    "@storybook/addon-links": "^8.0.10",
    "@storybook/addon-onboarding": "^8.0.10",
    "@storybook/blocks": "^8.0.10",
    "@storybook/react": "^8.0.10",
    "@storybook/react-vite": "^8.0.10",
    "@storybook/test": "^8.0.10",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.6",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "inquirer": "^9.2.20",
    "jsdom": "^24.0.0",
    "plop": "^4.0.1",
    "prettier": "3.2.5",
    "storybook": "^8.0.10",
    "stylelint": "^16.5.0",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-rem-over-px": "^1.0.1",
    "stylelint-selector-tag-no-without-class": "^3.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
